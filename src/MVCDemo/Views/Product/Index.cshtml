@model List<MVCDemo.Models.Product>

<h1>Product List</h1>

<table class="table">
    <tr>
        <td>
            Product ID
        </td>
        <td>Name</td>
        <td>Category</td>
        <td>Price</td>
    </tr>
@foreach (var item in Model)
{
    <tr>
        <td>@item.ProductID</td>
        <td>@item.Name</td>
        <td>@item.Category</td>
        <td>@item.Price</td>
    </tr>
}
</table>



